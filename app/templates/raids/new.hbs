<form class="form">
  <div class="row">
    <div class="col-md-6">
      <div class="form-group">
        <label for="raid-name">Name</label>
        {{input type="text" id="raid-name" value=name class="form-control" placeholder="Name"}}
      </div>
    </div>
  </div>
  <div class="form-group">
    <label for="raid-name">Guild</label>
    {{guild-select account=account guild=guild}}
  </div>
  <div class="checkbox">
    <label>
      {{input type="checkbox" id="raid-hidden" checked=hidden class="form-control"}} Hidden
    </label>
  </div>
  <div class="form-group">
    <label for="raid-name">Date</label>
    {{date-time date=date}}
  </div>
  <button class="btn btn-primary" {{action 'create'}}>Create</button>
</form>
